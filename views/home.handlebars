        <td>{{numRapport}}</td>
        <td>{{emetteur}}</td>
        <td>{{up}}</td>
        <td>{{dateEnregistrement}}</td>
        <td>{{codeArticle}}</td>
        <td>{{indice}}</td>
        <td>{{designationArticle}}</td>
        <td>{{qte}}</td>
        <td>{{numeroSerie}}</td>
        <td>{{anomalie}}</td>
        <td>{{fournisseur}}</td>
        <td>{{of}}</td>
        <td>{{commande}}</td>
        <td>{{etatAvancement}}</td>
        <td>{{dateCloture}}</td>
        <td>{{responsableAction}}</td>
        <td>{{coutUnitaire}}</td>
